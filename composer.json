{
    "name": "chrisrhymes/bulma-blade-ui",
    "description": "A set of Bulma components for Laravel Blade",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "chrisrhymes",
            "email": "csrhymes@gmail.com"
        }
    ],
    "require": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "BulmaBladeUi\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "BulmaBladeUi\\BulmaBladeUiServiceProvider"
            ]
        }
    },
    "require-dev": {
        "orchestra/testbench": "^6.11",
        "brianium/paratest": "^6.3",
        "nunomaduro/collision": "^5.4"
    },
    "scripts": {
        "test": "phpunit"
    }
}
